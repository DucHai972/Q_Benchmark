<questionnaire>
  <questions>
    <question id="date" type="open-ended">What is the date of the session?</question>
    <question id="sessionname" type="open-ended">What is the name of the session?</question>
    <question id="identification" type="mcq">
      Identification of self, role, and patient.
      <options>
        <option code="0">Requires direct prompting</option>
        <option code="1">Introduces themselves but only after a hint</option>
        <option code="2">Introduces themselves but information is incomplete</option>
        <option code="3">Introduces themselves and information is complete</option>
      </options>
    </question>
    <question id="situation" type="mcq">
      Identifies the main problem(s).
      <options>
        <option code="0">Unable to identify the main problem(s)</option>
        <option code="1">Identifies the main problem(s) after extended prompting</option>
        <option code="2">Identifies the main problem(s) with fewer prompts needed</option>
        <option code="3">Identifies and prioritizes the main problem(s) unprompted</option>
      </options>
    </question>
    <question id="backgroundHistory" type="mcq">
      Gives appropriate history.
      <options>
        <option code="0">History is unstructured or non-relevant</option>
        <option code="1">Relevant history but frequent clarification needed</option>
        <option code="2">Relevant history with few further questions needed</option>
        <option code="3">Comprehensive focused history</option>
      </options>
    </question>
    <question id="backgroundExamination" type="mcq">
      Gives appropriate examination/observations.
      <options>
        <option code="0">Observations are omitted or irrelevant</option>
        <option code="1">Observations reported but frequent clarification needed</option>
        <option code="2">Observations reported with few further questions needed</option>
        <option code="3">Comprehensive focused examination/observations</option>
      </options>
    </question>
    <question id="assessment" type="mcq">
      Makes logical assessment (correlates problem, history, exam, context).
      <options>
        <option code="0">No logical assessment</option>
        <option code="1">Assessment only after extended questioning</option>
        <option code="2">Assessment after minimal questioning</option>
        <option code="3">Comprehensive logical assessment without questioning</option>
      </options>
    </question>
    <question id="recommendationClearRecommendation" type="mcq">
      Makes a clear recommendation.
      <options>
        <option code="0">No recommendation</option>
        <option code="1">Clear recommendation only after extended questioning</option>
        <option code="2">Clear recommendation after minimal questioning</option>
        <option code="3">Clear comprehensive recommendation without questioning</option>
      </options>
    </question>
    <question id="recommendationGlobalRatingScale" type="mcq">
      Global Rating Scale (GRS) - How confident am I that I received an accurate picture of the patient?
      <options>
        <option code="0">Not at all confident</option>
        <option code="1">Confident but required extended questioning</option>
        <option code="2">Confident but required some further questioning</option>
        <option code="3">Confident and required little or no questioning</option>
      </options>
    </question>
    <question id="comments" type="open-ended">Comments on the session.</question>
  </questions>
